package CGWeek1;

public class DataTypeDemo {
	
	public static void main(String[] args)
	{
		byte a = 20;
		short b = 150;
		int c = 10000;
		long d = 1000000;
		char e = 'A';
		double g = 12345.6789;
		
		System.out.println("Int min value"+Integer.MIN_VALUE);
		System.out.println("Int max value"+Integer.MAX_VALUE);
		
	}

}
